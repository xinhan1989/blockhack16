#createassetPanel(hidden).panel
	h1 Asset Registration
	hr
	
	#innerWrap
		span.fa.fa-home(style="font-size: 350px; float:left;")
		
		span(style="float: left;")
			form(action="/createasset" method="POST")#createassetform
				legend 
					input(type="text" name="name")
					span.hint NAME
				legend 
					input(type="text" name="adrStreet")
					span.hint STREET ADDRESS
				legend 
					input(type="text" name="adrCity")
					span.hint CITY
				legend 
					input(type="number" name="adrPostcode")
					span.hint POSTCODE
				legend 
					select(name="adrState")
						option(value="NSW") New South Wales
						option(value="ACT") Australian Capital Territory
						option(value="VIC") Victoria
						option(value="QLD") Queensland
						option(value="SA") South Australia
						option(value="WA") Western Australia
						option(value="NT") Northern Territory
						option(value="TAS") Tasmania
					span.hint STATE
				legend 
					input(type="number" name="qty" value="100")
					span.hint TOKENS QTY
				legend 
					input(type="number" name="mktValue" placeholder="AUD")
					span.hint MARKET VALUE
				legend 
					input(type="url" name="urlWeb" urlType="web")
					span.hint WEB URL
				legend#groupImgUrl 
					legend 
						input(type="url" urlType="img").inpImgUrl
						span.hint IMAGE URL
							span:a.fa.fa-plus.btnAddImgUrl
				
				legend
					button(type="button" style="margin:5px;")#submitasset
						span.fa.fa-check &nbsp;&nbsp;
						span CREATE
				